    </owl:DatatypeProperty>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/P79_beginning_is_qualified_by -->


    <owl:DatatypeProperty rdf:about="http://www.cidoc-crm.org/cidoc-crm/P79_beginning_is_qualified_by">
